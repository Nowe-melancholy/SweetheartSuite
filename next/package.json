{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rimraf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "panda codegen",
    "codegen-user:win": "grpc_tools_node_protoc --plugin=protoc-gen-ts=%CD%/node_modules/.bin/protoc-gen-ts.cmd --proto_path=../go --js_out=import_style=commonjs,binary:types --grpc_out=grpc_js:types --ts_out=types ../go/pkg/User/presenter/user.proto",
    "codegen-todo:win": "grpc_tools_node_protoc --plugin=protoc-gen-ts=%CD%/node_modules/.bin/protoc-gen-ts.cmd --proto_path=../go --js_out=import_style=commonjs,binary:types --grpc_out=grpc_js:types --ts_out=types ../go/pkg/ToDo/presenter/todo.proto",
    "codegen:win": "npm run codegen-user:win && npm run codegen-todo:win"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.3",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "ts-proto": "^1.157.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.14.0",
    "@types/babel__core": "^7.20.1",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.11.3",
    "rimraf": "^5.0.1"
  }
}
